! = UXterm =
uxterm*termName: uxterm-256color
xterm*termName:  xterm-256color

xterm*foreground:  #FFFFFF
xterm*background:  #000000
uxterm*foreground:  #FFFFFF
uxterm*background:  #000000

xterm*faceName: xft:Px437 IBM VGA8:pixelsize=16

xterm*allowBoldFonts: false

! = URxvt =
!
URxvt.urgentOnBell: true
URxvt.scrollBar: false
URxvt.internalBorder: 5
! Fonts
URxvt*font: xft:{{ font }}:size={{ fontsize }}
URxvt.boldFont:

! Xft
xft.dpi:        96
xft.antialias:  true
xft.rgba:       rgb
xft.hinting:    true
xft.hintstyle:  hintslight

URxvt*.perl-ext-common: default,matcher
!URxvt*.url-launcher:    chromium
URxvt*.matcher.button:  1
URxvt*.transparent:     false

!= COLORS =
! Colors
URxvt.borderColor:  {{ bgc }}
URxvt.background:   {{ bgc }}
URxvt.foreground:   {{ fgc }}
! Black
URxvt.color0:       {{ n_black }}
URxvt.color8:       {{ b_black }}
! Red
URxvt.color1:       {{ n_red }}
URxvt.color9:       {{ b_red }}
! Green
URxvt.color2:       {{ n_green }}
URxvt.color10:      {{ b_green }}
! Yellow
URxvt.color3:       {{ n_yellow }}
URxvt.color11:      {{ b_yellow }}
! Blue
URxvt.color4:       {{ n_blue }}
URxvt.color12:      {{ b_blue }}
! Magenta
URxvt.color5:       {{ n_magenta }}
URxvt.color13:      {{ b_magenta }}
! Cyan
URxvt.color6:       {{ n_cyan }}
URxvt.color14:      {{ b_cyan }}
! White
URxvt.color7:       {{ n_white }}
URxvt.color15:      {{ b_white }}
