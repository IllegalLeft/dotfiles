#!/bin/bash
# Prints out a Boss CE-2 Chorus pedal in cyan using cat, ansi escape sequences
# and unicode characters (mostly cp437 related ones)


# Colors
# black    = 232
# darkgrey = 238
# white    = 255
# silver   = 250
# cyan     = 87

cat << EOF

[0m
[0m  [38;5;87m▄████████▄[0m
[0m  [38;5;87m██[38;5;250;48;5;232m◀▶[48;5;87m  [38;5;250;48;5;232m◀▶[38;5;87m██[0m
[0m  [38;5;87m█[48;5;87;38;5;232m────────[38;5;87m█[0m
[0;38;5;250m ▐[38;5;87m██[48;5;87;38;5;232mChorus[38;5;87m██[0;38;5;250m▌[0m
[0m  [38;5;87m█████[38;5;232;48;5;87mCE-2[38;5;87m█[0m
[0m  [38;5;87m█[38;5;238;48;5;232m  BOSS  [38;5;87m█[0m
[0m  [38;5;87m█[48;5;232m        [38;5;87m█[0m
[0m  [38;5;87m█[48;5;232m        [38;5;87m█[0m
[0m  [38;5;87m▀████████▀[0m
[0m[6C[1;30m ^[0m

EOF
