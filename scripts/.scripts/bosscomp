#!/bin/bash
# Prints out a Boss CS-3 Compressor pedal in cyan using cat, ansi escape
# sequences and unicode characters (mostly cp437 related ones)


# Colors
# black    = 232
# darkgrey = 238
# white    = 255
# silver   = 250
# blue     = 27

cat << EOF

[0m
[0m  [38;5;27m▄[38;5;232;48;5;27m▄▄▄▄▄▄▄▄[0;38;5;27m▄[0m
[0m  [38;5;27m█[38;5;250;48;5;232m o o o o[38;5;27m█[0m
[0m  [38;5;27m█[48;5;27;38;5;232m▀▀▀▀▀▀▀▀[38;5;27m█[0m
[0;38;5;250m ▐[38;5;27m[48;5;27;38;5;255mCompressor[38;5;27m[0;38;5;250m▌[0m
[0m  [38;5;27m█████[38;5;255;48;5;27mCS-3[38;5;27m█[0m
[0m  [38;5;27m█[38;5;238;48;5;232m  BOSS  [38;5;27m█[0m
[0m  [38;5;27m█[48;5;232m        [38;5;27m█[0m
[0m  [38;5;27m█[48;5;232m        [38;5;27m█[0m
[0m  [38;5;27m▀████████▀[0m
[0m[6C[1;30m ^[0m

EOF
